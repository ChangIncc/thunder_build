{
  "name": "drupal/shariff",
  "description": "Integrating Shariff library and providing some options.",
  "type": "drupal-module",
  "license": "GPL-2.0+",
  "authors": [
    {
      "name": "Christian Ziegler",
      "email": "christian@acolono.com",
      "role": "Maintainer"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.drupal-composer.org"
    },
    {
      "type": "package",
      "package": {
        "name": "heiseonline/shariff",
        "version": "master",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/heiseonline/shariff/archive/master.zip",
          "type": "zip"
        },
        "require": {
          "composer/installers": "~1.0"
        }
      }
    }
  ],
  "require": {
    "heiseonline/shariff": "master"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "libraries/{$name}": ["type:drupal-library"]
    }
  }
}